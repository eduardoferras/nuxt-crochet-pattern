{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"baseUrl": "src",
		"paths": {
			"@app/*": ["app/*"],
			"@components/*": ["components/*"],
			"@interfaces/*": ["interfaces/*"],
			"@hooks/*": ["hooks/*"],
			"@services/*": ["services/*"],
			"@/*": ["./*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
