{
	"extends": "./.nuxt/tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@composables/*": ["src/composables/*"],
			"@schemas/*": ["src/schemas/*"],
			"@validations/*": ["src/validations/*"],
			"@/*": ["src/*"]
		},
		"strict": true
	}
}
