{
	"extends": "./.nuxt/tsconfig.json",
	"compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			"@components/*": ["components/*"],
			"@composables/*": ["composables/*"],
			"@validations/*": ["validations/*"],
			"@interfaces/*": ["interfaces/*"],
			"@services/*": ["services/*"],
			"@utils/*": ["utils/*"],
			"@/*": ["*"]
		},
		"strict": true
	}
}
